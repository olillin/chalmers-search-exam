{
    "name": "chalmers-search-exam",
    "version": "1.0.1",
    "description": "Search exam dates at Chalmers",
    "files": [
        "/lib",
        "/src"
    ],
    "type": "commonjs",
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "exports": {
        ".": {
            "types": "./lib/index.d.ts",
            "default": "./lib/index.js"
        }
    },
    "bin": {
        "cthexam": "./lib/cli.js"
    },
    "scripts": {
        "build": "rimraf lib && tsc",
        "test": "jest",
        "coverage": "jest --coverage",
        "lint": "eslint .",
        "format": "prettier . --write -l"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/olillin/chalmers-search-exam.git"
    },
    "keywords": [
        "chalmers",
        "exam",
        "cli",
        "library"
    ],
    "author": "Oli<oli@olillin.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/olillin/chalmers-search-exam/issues"
    },
    "homepage": "https://github.com/olillin/chalmers-search-exam#readme",
    "devDependencies": {
        "@eslint/compat": "^2.0.1",
        "@eslint/js": "^9.39.2",
        "@types/jest": "^30.0.0",
        "eslint": "^9.39.2",
        "eslint-plugin-jsdoc": "^62.4.1",
        "jest": "^30.2.0",
        "prettier": "^3.8.1",
        "rimraf": "^6.1.2",
        "ts-jest": "^29.4.6",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.54.0"
    },
    "dependencies": {
        "chalk": "^5.6.2"
    }
}
